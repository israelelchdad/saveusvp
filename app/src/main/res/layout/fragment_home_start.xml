<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <fragment
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"


        android:layout_centerInParent="true">

    <View
        android:layout_width="238dp"
        android:layout_height="238dp"
        android:background="@drawable/circle_butten_com_out"
        android:layout_centerInParent="true"/>
    <View
        android:layout_width="205dp"
        android:layout_height="205dp"
        android:background="@drawable/circle_butten_com_middle"
        android:layout_centerInParent="true"/>
        <View
            android:layout_width="190dp"
            android:layout_height="190dp"
            android:id="@+id/f_home_start_onclick_location"
            android:background="@drawable/circle_butten_com_into"
            android:layout_centerInParent="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/f_home_start_text_come"
            android:layout_centerInParent="true"
            android:text="@string/f_homestart_icoming"
            android:textColor="#ffffff"
            android:textSize="40sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="@string/f_home_start_start_regis"
            android:textColor="#ffffff"
            android:layout_centerInParent="true"
            android:layout_below="@+id/f_home_start_text_come"
            android:textSize="20sp"
            />



    </RelativeLayout>


</RelativeLayout>